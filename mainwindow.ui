QMenuBar {  
	background-color: rgb(24, 25, 25); /*rgb(17, 17, 18);*/
	spacing: 5px; /*spacing between menu bar items */
	border-bottom: 1px solid rgb(65, 65, 65);
	color:rgb(225, 225, 225);/*color font*/
}

QMenuBar::item {  
	padding: 2px 10px;
	background: transparent;  
	/*border-radius: 4px;*/
}

QMenuBar::item:selected, pressed { /* when selected using mouse or keyboard */  
	background-color: rgb(44, 43, 50);
}

/*-----------------------------------------------------------------------------*/

QMenu {  
	
	color: rgb(199, 199, 199);
	background-color: rgb(44, 43, 50); /* sets background of the menu */  
	border:none;
}

QMenu::item {
	background-color: transparent;
}

QMenu::item:selected { /* when user selects item using mouse or keyboard */
	 background-color: rgb(120, 117, 130);
}

QMenu::separator { 
 	height: 1px; 
	background: rgb(78, 78, 79);  
	margin: 1px 5px;
}

/*-------------------------------------------------------------------------------------------------------*/

QPushButton{
	color: white;
	background-color: rgb(44, 43, 50);
	border: none;
}

QPushButton::hover{
	background-color:rgb(43, 50, 76);
}

QPushButton::pressed{
	background-color:rgb(78, 78, 79);
}

